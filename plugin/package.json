{
  "name": "brain-plugin",
  "version": "0.0.1",
  "description": "",
  "main": "",
  "scripts": {
    "start": "echo 'build or test'",
    "dev": "watch 'npm run build' ./src",
    "test": "nyc mocha",
    "test.watch": "watch 'npm test' ./src",
    "coverage": "npm test && nyc report --reporter=lcov",
    "build": "npm run lint && npm test && node fuse.js",
    "lint": "tslint -c tslint.json src/**/*"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "mhcurylo",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/chrome": "0.0.47",
    "@types/mocha": "^2.2.40",
    "@types/sinon": "^2.1.2",
    "chai": "^4.1.0",
    "fuse-box": "^2.2.1",
    "mocha": "^3.2.0",
    "mock-socket": "^6.0.4",
    "nyc": "^11.1.0",
    "sinon": "^2.1.0",
    "source-map-support": "^0.4.14",
    "ts-node": "^3.0.2",
    "tslint": "^5.0.0",
    "typescript": "^2.4.2",
    "watch": "^1.0.2"
  }
}
